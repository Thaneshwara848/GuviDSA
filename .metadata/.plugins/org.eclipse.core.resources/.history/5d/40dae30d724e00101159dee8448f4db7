package comA;
public class Demo4 {
	public static void main(String[] args) {
		byte a=127;
//				size  1 byte : 8 bit 
					// 2 pow 8 : 256 
					//  - half and + half 
					// 256 / 2 = 128
					// - 128 to + 127 
		System.out.println(a);	
		short s = 32767;
		System.out.println(s); //
	}
}
byte = 1 = 8 bit
short= 2 = 16 bit 
int  = 4 = 32 bit 
long = 8 = 64 bit 
float= 4 = 32 bit 
double 8 = 64 bit 
	
		